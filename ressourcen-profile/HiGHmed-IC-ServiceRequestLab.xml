      <path value="ServiceRequest.identifier.type" />
      <min value="1" />
      <patternCodeableConcept>
        <coding>
          <system value="http://terminology.hl7.org/CodeSystem/v2-0203" />
          <code value="FILL" />
        </coding>
      </patternCodeableConcept>
      <mustSupport value="true" />
