      <path value="Specimen.collection.collector" />
      <label value="Identifikator des Probennehmers" />
      <short value="Die Person oder Organisation die für die Entnahme der Probe verantwortlich war." />
      <mapping>
        <identity value="v2" />
        <map value="OBR-10" />
        <comment value="v 2.3" />
